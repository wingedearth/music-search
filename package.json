{
  "name": "music-search",
  "version": "1.2.9",
  "description": "A search interface for music",
  "main": "index.js",
  "engines": {
    "node": "22.16.0"
  },
  "scripts": {
    "bump": "npm version patch && git push && git push --tags",
    "clean": "rimraf build",
    "frontend": "node ./node_modules/webpack/bin/webpack --config webpack.config.frontend.js",
    "frontend:dev": "node ./node_modules/webpack/bin/webpack --config webpack.config.frontend.js --watch",
    "server": "ENVIRONMENT=production node ./node_modules/webpack/bin/webpack --config ./webpack.config.backend.js && node ./build/server.js",
    "server:dev": "ENVIRONMENT=development node ./node_modules/webpack/bin/webpack --config ./webpack.config.backend.js --watch",
    "start": "npm run clean && npm run frontend && ENVIRONMENT=production npm run server",
    "start:dev": "npm run clean && npm run frontend:dev & npm run server:dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wingedearth/music-search.git"
  },
  "author": "Andrew A. Anissi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wingedearth/music-search/issues"
  },
  "homepage": "https://github.com/wingedearth/music-search#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "base-64": "^1.0.0",
    "body-parser": "^1.20.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "file-loader": "^6.2.0",
    "fs": "0.0.1-security",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.7.6",
    "mongodb": "^4.17.1",
    "mongoose": "^6.12.0",
    "nodemon-webpack-plugin": "^4.8.2",
    "path": "^0.12.7",
    "prop-types": "^15.8.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.2",
    "superagent": "^8.0.9",
    "url-loader": "^4.1.1",
    "webpack-omit-js-for-css-plugin": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/runtime": "^7.28.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.3.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.57.0",
    "eslint-config": "^0.3.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.10.0",
    "eslint-webpack-plugin": "^4.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "mongodb-memory-server": "^10.1.4",
    "prettier": "^2.8.8",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
