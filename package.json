{
  "name": "music-search",
  "version": "1.2.3",
  "description": "A search interface for music",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build",
    "frontend": "node ./node_modules/webpack/bin/webpack --config webpack.config.frontend.js",
    "frontend:dev": "node ./node_modules/webpack/bin/webpack --config webpack.config.frontend.js --watch",
    "server": "ENVIRONMENT=production node ./node_modules/webpack/bin/webpack --config ./webpack.config.backend.js && node ./build/server.js",
    "server:dev": "ENVIRONMENT=development node ./node_modules/webpack/bin/webpack --config ./webpack.config.backend.js --watch",
    "start": "npm run clean && npm run frontend && ENVIRONMENT=production npm run server",
    "start:dev": "npm run clean && npm run frontend && npm run server:dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wingedearth/music-search.git"
  },
  "author": "Andrew A. Anissi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wingedearth/music-search/issues"
  },
  "homepage": "https://github.com/wingedearth/music-search#readme",
  "dependencies": {
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "base-64": "^0.1.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "lodash": "^4.17.10",
    "mini-css-extract-plugin": "^0.4.1",
    "mongodb": "^3.1.0",
    "mongoose": "^5.1.7",
    "nodemon-webpack-plugin": "^4.0.3",
    "path": "^0.12.7",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "rimraf": "^2.6.2",
    "superagent": "^3.8.3",
    "webpack-omit-js-for-css-plugin": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config": "^0.3.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.1",
    "eslint-plugin-react": "^7.10.0",
    "node-sass": "^4.9.2",
    "prettier": "^1.13.7",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.14.0",
    "webpack-assets-manifest": "^3.0.1",
    "webpack-cli": "^3.0.8",
    "webpack-node-externals": "^1.7.2"
  }
}
