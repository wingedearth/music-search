{
  "name": "music-search",
  "version": "1.0.0",
  "description": "A search interface for music",
  "main": "index.js",
  "scripts": {
    "build:server": "npx babel src/server --out-dir build/server",
    "clean": "rimraf build",
    "prebuild:server": "npm run clean",
    "server": "node ./build/server/server.js",
    "server:dev": "nodemon ./build/server/server.js",
    "start": "npm run build:server && npm run server",
    "start:dev": "npm run build:server && npm run server:dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wingedearth/music-search.git"
  },
  "author": "Andrew A. Anissi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wingedearth/music-search/issues"
  },
  "homepage": "https://github.com/wingedearth/music-search#readme",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "lodash": "^4.17.10",
    "path": "^0.12.7",
    "rimraf": "^2.6.2",
    "webpack": "^4.13.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0"
  }
}
